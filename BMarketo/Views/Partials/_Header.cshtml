@{
    var headermenu = headerService.GetLatest();
}


<header>
    <div class="container g-0 ">
       @* <div class="logo"> <img class="logo" src="https://saxeit.se/images/logo.svg" alt="logo"></div>*@

        <div class="logo g-0"> <img class="logo" src="@headermenu.Logo" alt="logo"></div>

        <nav id="menu" class="menu-links">
            <a class="link" asp-controller="Home" asp-action="Index">@headermenu.HomeLink</a>
            <a class="link" asp-controller="Products" asp-action="Index">@headermenu.ProductsLink</a>
            <a class="link" asp-controller="Contacts" asp-action="Index">@headermenu.ContactsLink</a>
        </nav>
        <nav class="menu-icons">
            <a class="link" asp-controller="Products" asp-action="Search"><i class="fa-regular fa-search"></i></a>
            <a class="link" asp-controller="Account" asp-action="Login">@headermenu.LoginLink</a>
            <a class="link" asp-controller="Cart" asp-action="Index"><i class="fa-solid fa-cart-shopping"></i></a>
        </nav>

        <div class="toggle">
            <button class="btn-toggle btn-outline-white" data-option="toggle" data-target="#menu">
                <i id="icon" class="fa-regular fa-bars"></i>
            </button>
        </div>
    </div>
</header>
<!-- end header  -->