@model List<GridCollectionSmallCardsItemViewModel>

@foreach (var item in Model)
{
    <a asp-route-id="@item.Id" class="gridcollection-item">
        <div class="card">
            <img src="data:image/png;base64,@Convert.ToBase64String(item.Image)" class="card-img-top" alt="...">
        </div>
    </a>
}
