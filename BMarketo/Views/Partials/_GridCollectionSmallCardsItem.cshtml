@model List<GridCollectionSmallCardsItemViewModel>



@foreach (var item in Model)
{
    <a href="@item.Url" class="smallcards_collection">
        <div class="card">
            @foreach (var image in item.Images)
            {
                <img src="data:image/png;base64,@Convert.ToBase64String(image)" class="card-img-top" alt="...">
            }
        </div>
    </a>
}

